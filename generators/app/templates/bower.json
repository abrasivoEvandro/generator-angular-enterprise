{
  "name": "<%= appName %>",
  "version": "0.0.0",
  "main": "<%= clientSideFolder %>index.html",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "angular": "~1.4",
    "angular-mocks": "~1.4.7",
<% if (bootstrap) { -%>
    "bootstrap-sass-official": "~3.3.4",
    "angular-bootstrap": "~0.13.0",
    "font-awesome": "~4.0",
<% } -%>
<% if (uirouter) { -%>
    "angular-ui-router": "~0.2.15"
<% } else { -%>
    "angular-route": "~1.4.0"
<% } -%>
  }
}
